{
  "metadata": [
    {
      "src": [],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": ["**/*.{md,yml}"],
        "exclude": ["_site/**"]
      }
    ],
    "resource": [
      {
        "files": ["images/**"]
      }
    ],
    "output": "_site",
    "template": ["default", "modern"],
    "globalMetadata": {
      "_appFaviconPath": "images/altamkp.ico",
      "_appFooter": "Copyright (c) 2024 <a href='https://github.com/altamkp'>altamkp</a>",
      "_appLogoPath": "images/altamkp_title.png",
      "_appName": "altamkp",
      "_appTitle": "altamkp",
      "_enableSearch": true,
      "_disableContribution": true,
      "pdf": false
    }
  }
}
