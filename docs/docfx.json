{
    "metadata": [
      {
        "src": [],
        "dest": "api"
      }
    ],
    "build": {
      "content": [
        {
          "files": ["**/*.{md,yml}"],
          "exclude": ["_site/**"]
        }
      ],
      "resource": [
        {
          "files": ["images/**"]
        }
      ],
      "output": "_site",
      "template": ["default", "modern"],
      "globalMetadata": {
        "_appFaviconPath": "images/altamkp.ico",
        "_appFooter": "Copyright (c) 2024 <a href='https://altamkp.github.io'>altamkp</a>",
        "_appLogoPath": "images/altamkp.png",
        "_appName": "altamkp",
        "_appTitle": "altamkp",
        "_enableSearch": true,
        "pdf": false
      }
    }
  }
  